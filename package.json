{
  "name": "wwcodemanila-womenwhowp2018",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "gh-pages": "^2.0.1",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "reactstrap": "^6.5.0",
    "sass-loader": "^7.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/styles/scss -o src/assets/styles/css",
    "watch-css": "yarn build-css && node-sass-chokidar src/assets/styles/scss -o src/assets/styles/css --watch --recursive",
    "dev": "concurrently \"yarn watch-css\" \"yarn start\"",
    "start": "NODE_PATH=src/ react-scripts start",
    "predeploy": "yarn build-css && react-scripts build",
    "deploy": "gh-pages -b master -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "homepage": "https://wwcodemanila.github.io/WWCodeManila-WomenWhoWP2018/"
}
